{
	"name": "speech2code",
	"displayName": "Spoken",
	"description": "Code With Voice - The default VSCode extension for Speech2Code",
	"publisher": "pedroaugusto",
	"author": {
		"name": "Pedro Augusto Silva",
		"email": "pedro42augusto@gmail.com",
		"url": "https://github.com/pedrooaugusto"
	},
	"version": "0.0.2-beta.1",
	"icon": "icon.png",
	"license": "GPL-3.0-or-later",
	"repository": {
		"type": "git",
		"url": "https://github.com/pedrooaugusto/speech-to-code.git"
	},
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "spoken.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc-watch  --onSuccess \"bash ./tools.sh reload\" -p ./",
		"pretest": "npm run compile",
		"lint": "eslint src --ext ts",
		"test": "jest \"out/__tests__/(.*).test.js$\"",
		"vscode-install": "bash ./tools.sh install",
		"build": "rm -rf *.vsix && vsce package"
	},
	"devDependencies": {
		"@types/jest": "^26.0.22",
		"@types/node": "^12.11.7",
		"@types/node-ipc": "^9.1.3",
		"@types/vscode": "^1.54.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"jest": "^26.6.3",
		"tsc-watch": "^4.2.9",
		"typescript": "^4.1.3",
		"vsce": "^1.96.1"
	},
	"dependencies": {
		"node-ipc": "9.1.4"
	}
}
